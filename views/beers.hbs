<article class="all-beers">
    <div>
        {{#each beersFromApi}}
        <h1>{{this.name}}</h1>
        <div class="description">{{this.description}}</div>
        <div class="tag-line">{{this.tagline}}</div>
        <div><img class="image" src="{{this.image_url}}" alt=""></div>
        {{/each}}
    </div>
</article>